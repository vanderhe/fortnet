Network = BPNN {

  Hidden = 2 2
  Activation = 'sigmoid'

}

Mapping = ACSF {

  NRadial = 5
  NAngular = 4
  RCut = 4.0
  Standardization = Yes

}

Training = SD {

  Threshold = 1e-20
  NIterations = 1
  NPrintout = 1
  NSaveNet = 1
  MinDisplacement = 1e-10
  MaxDisplacement = 5e-02
  LearningRate = 0.01
  Loss = 'rms'

}

Data {

  Dataset = 'fnetdata.xml'
  Validset = 'fnetvdata.xml'

  Standardization = No

  NetstatFiles = Type2FileNames {
    Prefix = "./"
    Suffix = ".net"
    LowerCaseTypeName = No
  }

}

External {

  Features = 1 2 3

}

Options {

  ReadNetStats = No
  Mode = 'train'

  RandomSeed = 123456

}