Training = SD {

  Threshold = 1e-20
  NIterations = 1
  NPrintout = 1
  NSaveNet = 1
  MinDisplacement = 1e-10
  MaxDisplacement = 5e-02
  LearningRate = 0.01
  Loss = 'rms'

}

Data {

  Dataset = 'training_data'
  Standardization = No

  NetstatFiles = Type2FileNames {
    Prefix = "./"
    Suffix = ".net"
    LowerCaseTypeName = No
  }

}

Options {

  ReadNetStats = Yes
  Mode = 'train'

  RandomSeed = 123456

}